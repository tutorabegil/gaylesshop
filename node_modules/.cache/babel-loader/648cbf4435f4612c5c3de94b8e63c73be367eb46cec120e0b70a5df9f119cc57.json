{"ast":null,"code":"export default {\n  data() {\n    return {\n      searchQuery: '',\n      products: [{\n        id: 1,\n        name: 'Buko Pie',\n        price: 25,\n        image: '/img/buko pie.jpg'\n      }, {\n        id: 2,\n        name: 'Banana Cue',\n        price: 27,\n        image: '/img/bananacue.jpg'\n      }, {\n        id: 3,\n        name: 'Empenada',\n        price: 29,\n        image: '/img/empenada.jpg'\n      }, {\n        id: 4,\n        name: 'Maruya',\n        price: 26,\n        image: '/img/maruya.jpg'\n      }, {\n        id: 5,\n        name: 'Kutsinta',\n        price: 20,\n        image: '/img/kutsinta.jpg'\n      }, {\n        id: 6,\n        name: 'Puto',\n        price: 22,\n        image: '/img/puto.jpg'\n      }, {\n        id: 7,\n        name: 'Pansit',\n        price: 20,\n        image: '/img/pansit.jpg'\n      }, {\n        id: 8,\n        name: 'Turon',\n        price: 15,\n        image: '/img/turon.jpg'\n      }, {\n        id: 9,\n        name: 'Irish Coffee',\n        price: 170,\n        image: '/img/img9.jpg'\n      }]\n    };\n  },\n  computed: {\n    filteredProducts() {\n      if (!this.searchQuery.trim()) {\n        return this.products; // Return all products if search query is empty\n      }\n      const query = this.searchQuery.toLowerCase();\n      return this.products.filter(product => product.name.toLowerCase().includes(query));\n    }\n  },\n  methods: {\n    addToCart(product) {\n      this.$emit('add-to-cart', product);\n    }\n  }\n};","map":{"version":3,"names":["data","searchQuery","products","id","name","price","image","computed","filteredProducts","trim","query","toLowerCase","filter","product","includes","methods","addToCart","$emit"],"sources":["C:\\mytemplate\\src\\components\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1 class=\"product-title\" style=\"display: flex; font-size: 70px; margin-top: 20%; justify-content: center; align-items: center;;\">MINATO</h1>\r\n    <h1 class=\"product-title\" style=\"display: flex; font-size: 50px; margin-top:-5%; justify-content: center; align-items: center;;\">Miryenda Natin 'To</h1>\r\n    <div class=\"search-container\">\r\n      <h3 class=\"search-title\">LIST OF MINATO FOODS</h3>\r\n      <input type=\"text\" v-model=\"searchQuery\" placeholder=\"Type here...\">\r\n    </div>\r\n    <div class=\"product-container\">\r\n      <ul class=\"product-list\">\r\n        <li v-if=\"filteredProducts.length === 0\" class=\"no-results\">No matching products found.</li>\r\n        <li v-for=\"product in filteredProducts\" :key=\"product.id\" class=\"product-item\">\r\n          <div class=\"product-info\">\r\n            <img :src=\"product.image\" alt=\"Product Image\" class=\"product-image\">\r\n            <div>\r\n              <p class=\"product-name\">{{ product.name }}</p>\r\n              <p class=\"product-price\">Php {{ product.price }}</p>\r\n            </div>\r\n          </div>\r\n          <button @click=\"addToCart(product)\" class=\"add-to-cart-btn\">Add to Cart</button>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      products: [\r\n        { id: 1, name: 'Buko Pie', price: 25, image: '/img/buko pie.jpg' },\r\n        { id: 2, name: 'Banana Cue', price: 27, image: '/img/bananacue.jpg' },\r\n        { id: 3, name: 'Empenada', price: 29, image: '/img/empenada.jpg' },\r\n        { id: 4, name: 'Maruya', price: 26, image: '/img/maruya.jpg' },\r\n        { id: 5, name: 'Kutsinta', price: 20, image: '/img/kutsinta.jpg' },\r\n        { id: 6, name: 'Puto', price: 22, image: '/img/puto.jpg' },\r\n        { id: 7, name: 'Pansit', price: 20, image: '/img/pansit.jpg' },\r\n        { id: 8, name: 'Turon', price: 15, image: '/img/turon.jpg' },\r\n        { id: 9, name: 'Irish Coffee', price: 170, image: '/img/img9.jpg' }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    filteredProducts() {\r\n      if (!this.searchQuery.trim()) {\r\n        return this.products; // Return all products if search query is empty\r\n      }\r\n      const query = this.searchQuery.toLowerCase();\r\n      return this.products.filter(product =>\r\n        product.name.toLowerCase().includes(query)\r\n      );\r\n    }\r\n  },\r\n  methods: {\r\n    addToCart(product) {\r\n      this.$emit('add-to-cart', product);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.product-title {\r\n \r\n  color: orangered;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.search-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.search-title {\r\n  font-size: 20px;\r\n  color: #000; /* black color */\r\n  margin-bottom: 10px;\r\n}\r\n\r\n\r\ninput[type=\"text\"] {\r\n  width: 500px;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.input-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.product-container {\r\n  background-color:  black;\r\n  opacity: 80%;\r\n  margin-top: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n  padding: 20px;\r\n}\r\n\r\n.product-list {\r\n  list-style: none;\r\n  padding: 0;\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr); \r\n  gap: 20px; \r\n}\r\n\r\n.product-item {\r\n  background-color: transparent;\r\n  opacity: 120%;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n  box-shadow: 0px 0px 10px rgba(255, 0, 0, 0.708);\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.product-info {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.product-image {\r\n  width: 80px;\r\n  height: 80px;\r\n  border-radius: 10px;\r\n  border: 2px solid transparent;\r\n  margin-right: 20px;\r\n}\r\n\r\n.product-name {\r\n  color: #fff;\r\n}\r\n\r\n.product-price {\r\n  color: #fff;\r\n}\r\n\r\n.add-to-cart-btn {\r\n  background-color: #ff0;\r\n  color: #000;\r\n  border: none;\r\n  padding: 8px 15px;\r\n  font-size: 12px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.add-to-cart-btn:hover {\r\n  background-color: #ffc107;\r\n  color: #000;\r\n}\r\n\r\n.no-results {\r\n  color: #ff0;\r\n  font-style: italic;\r\n}\r\n</style>\r\n"],"mappings":"AA2BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,CACR;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAoB,CAAC,EAClE;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAqB,CAAC,EACrE;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAoB,CAAC,EAClE;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAkB,CAAC,EAC9D;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAoB,CAAC,EAClE;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,MAAM;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAgB,CAAC,EAC1D;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAkB,CAAC,EAC9D;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,OAAO;QAAEC,KAAK,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAiB,CAAC,EAC5D;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE,GAAG;QAAEC,KAAK,EAAE;MAAgB;IAEtE,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAAC,IAAI,CAACP,WAAW,CAACQ,IAAI,CAAC,CAAC,EAAE;QAC5B,OAAO,IAAI,CAACP,QAAQ,EAAE;MACxB;MACA,MAAMQ,KAAI,GAAI,IAAI,CAACT,WAAW,CAACU,WAAW,CAAC,CAAC;MAC5C,OAAO,IAAI,CAACT,QAAQ,CAACU,MAAM,CAACC,OAAM,IAChCA,OAAO,CAACT,IAAI,CAACO,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACJ,KAAK,CAC3C,CAAC;IACH;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,SAASA,CAACH,OAAO,EAAE;MACjB,IAAI,CAACI,KAAK,CAAC,aAAa,EAAEJ,OAAO,CAAC;IACpC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}